{
  "extends": [
		"next/core-web-vitals",
		"airbnb",
		"airbnb-typescript"
	],
	"parserOptions": {
		"project": "./tsconfig.json"
	},
	"rules": {
		"linebreak-style": 0,
		"react/jsx-props-no-spreading": "off",
		"no-console": ["warn", { "allow": ["warn", "error"] }],
		"react/react-in-jsx-scope": "off",
		"max-len": "warn"
	}
}
